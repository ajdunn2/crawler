{
    "name": "dmelearn/crawler",
    "description": "Crawl all internal links found on a website. Originally based on spatie/crawler 2.x",
    "homepage": "https://github.com/ajdunn2/crawler",
    "license": "MIT",
    "require": {
        "php": ">=7.0",
        "symfony/dom-crawler": "^3.0|^4.0",
        "guzzlehttp/guzzle": "^6.3",
        "tightenco/collect": "^5.3",
        "nicmart/tree": "^0.2.7",
        "spatie/browsershot": "^2.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5",
        "symfony/var-dumper": "^3.3"
    },
    "autoload": {
        "psr-4": {
            "Dmelearn\\Crawler\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dmelearn\\Crawler\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.0.x-dev"
        }
    },
    "config": {
        "preferred-install": "dist",
        "platform": {
            "php": "7.0.0"
        }
    }
}
